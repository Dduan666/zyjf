            function showul() {
                var hiorc = document.getElementById("hiorc").value;
                if (hiorc == "1") {
                    document.getElementById("otherLinks").style.display = "block";
                    document.getElementById("orc").innerHTML = "����&nbsp;��";
                    document.getElementById("hiorc").value = "0";
                } else {
                    document.getElementById("otherLinks").style.display = "none";
                    document.getElementById("orc").innerHTML = "չ��&nbsp;��";
                    document.getElementById("hiorc").value = "1";
                }
            }
