        <script src="../Scripts/move.js"></script>
		
        <script>
			window.onload=function()
			{
				/*��ʾ��Ϣ��ʼ*/
				var hz_tishiz=document.getElementById("hz_tishiz")
				var hzall=document.getElementById("hzall");
				move(hz_tishiz,{height:40});
				move(hzall,{marginTop:40});
				var hz_closz=document.getElementById("hz_closz");
				hz_closz.onclick=function()
				{
				    move(hzall,{marginTop:0});
					move(hz_tishiz,{height:0});
				   
				}
				/*��ʾ��Ϣ����*/
			  var  hz_oul= document.getElementById("hz_oul");
			  var  hz_oLi=hz_oul.getElementsByTagName("li");
			  var  hz_banner_tit=document.getElementById("hz_banner_tit");
			  var  hz_oDiv=hz_banner_tit.getElementsByTagName("div");
			  var  hz_banner_left=document.getElementById("hz_banner_left");
			  var  hz_banner_right=document.getElementById("hz_banner_right"); 
			  var time=null;
				  var num=0;
				  
				  for(var i=0;i <hz_oDiv.length;i++ )
				  {
					  hz_oDiv[i].index=i
					  hz_oDiv[i].onclick=function()
					  {
						 num=this.index;
						 acd(num);  
					  } 
				  }
				   function acd(nun)
				   {
					   
					   for(var i=0;i<hz_oDiv.length;i++ )
						   {
								hz_oDiv[i].className='';
								hz_oLi[i].style.display='none';   
						   }
						  hz_oDiv[num].className='active';
						  hz_oLi[num].style.display='block';
				  }
					 
					 clearInterval(time);
					 
					 
					 hz_banner_left.onclick=function()
					 {
						 clearInterval(time);
					 
						 num--;
						 if(num==-1)
						 {
						   num=hz_oDiv.length-1;	 
						 }
						 
						acd(num);  	 
					 }
					 
					 hz_banner_right.onclick=function()
					 {
						 clearInterval(time);
					 
						 num++;
						 if(num==hz_oDiv.length)
						 {
						   num=0	 
						 }
						 
						acd(num);  	 
					 }
					 
					var time= setInterval(
					 
					 function()
					 {
						 num++;
						 if(num==hz_oDiv.length)
						 {
						   num=0	 
						 }
						 
						acd(num); 
						
					 }
					 
					 ,5000)
			}
			
			</script>
        
